{
  "name": "comigo",
  "version": "1.0.0",
  "description": "Frontend for Comigo.",
  "license": "MIT",
  "browserslist": "> 0.5%, last 2 versions, not dead",
  "scripts": {
    "fmt": "prettier --write .",
    "build": "parcel build ./router/assets/scripts.js ./router/assets/styles.scss --dist-dir ./router/static",
    "dev": "parcel build ./router/assets/scripts.js ./router/assets/styles.scss --dist-dir ./router/static --no-optimize",
    "watch": "parcel watch ./router/assets/scripts.js ./router/assets/styles.scss --dist-dir ./router/static --no-optimize"
  },
  "dependencies": {
    "@alpinejs/morph": "^3.14.1",
    "@alpinejs/persist": "^3.14.1",
    "@tailwindcss/forms": "latest",
    "@tailwindcss/typography": "latest",
    "alpinejs": "latest",
    "flowbite": "^2.5.2",
    "htmx.org": "latest",
    "i18next": "^23.16.0",
    "i18next-browser-languagedetector": "^8.0.0",
    "screenfull": "^6.0.2",
    "tailwindcss": "latest",
    "tw-colors": "^3.3.2"
  },
  "devDependencies": {
    "@parcel/transformer-sass": "latest",
    "autoprefixer": "latest",
    "parcel": "latest",
    "postcss": "latest",
    "prettier": "latest"
  }
}
